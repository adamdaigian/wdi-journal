<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>

<% @articles.each do |article| %>
  <p>
    <%= link_to(article.title, "/article/#{article.id}") %> -
    <%= link_to("Edit", "/articles/edit/#{article.id}") %> -
    <%= link_to("Delete", "/articles/delete/#{article.id}", :method => :delete) %>
  </p>
<% end %>